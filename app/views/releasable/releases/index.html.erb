<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Key</th>
      <th>User IDs</th>
      <th>Active</th>
      <th>Created At</th>
      <th>Updated At</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @releases.each do |release| %>
      <tr>
        <td><%= release.id %></td>
        <td><%= release.key %></td>
        <td><%= release.user_ids %></td>
        <td><%= release.user_ids %></td>
        <td><%= release.active %></td>
        <td><%= release.created_at %></td>
        <td><%= release.updated_at %></td>
        <td><%= link_to "Delete", release, method: :delete, data: { confirm: "Are you sure?" } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
